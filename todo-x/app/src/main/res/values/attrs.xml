<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="SummaryItem">
        <attr name="si_icon" format="reference" />
        <attr name="si_title" format="string" />
        <attr name="si_description" format="string" />
        <attr name="si_iconTint" format="reference" />
        <attr name="si_iconBackground" format="reference" />
    </declare-styleable>

    <declare-styleable name="ContextButton">
        <attr name="cb_icon" format="reference" />
        <attr name="cb_iconTint" format="reference" />
        <attr name="cb_text" format="string" />
        <attr name="cb_textColor" format="reference" />
        <attr name="cb_backgroundTint" format="reference" />
        <attr name="cb_strokeColor" format="reference" />
    </declare-styleable>

    <declare-styleable name="EmptyView">
        <attr name="ev_icon" format="reference" />
        <attr name="ev_description" format="string" />
        <attr name="ev_action" format="string" />
    </declare-styleable>

    <declare-styleable name="AboutReference">
        <attr name="ar_text" format="string" />
        <attr name="ar_link" format="string" />
        <attr name="ar_icon" format="reference" />
    </declare-styleable>

    <declare-styleable name="ItemEditItem">
        <attr name="iei_text" format="string" />
    </declare-styleable>

</resources>